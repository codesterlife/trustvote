    "title": "ElectionFactory",
    "details": "Factory contract for deploying new Election contracts",
    "methods": {
      "createElection(string,uint256,uint256,uint256[])": {
        "details": "Create a new election contract and store its address",
        "params": {
          "_endTime": "End time of the election as Unix timestamp",
          "_positionIds": "Array of position IDs to include in the election",
          "_startTime": "Start time of the election as Unix timestamp",
          "_title": "Title of the election"
        },
        "returns": {
          "_0": "The address of the created election contract"
        }
      },
      "getAllElections()": {
        "details": "Get all election addresses",
        "returns": {
          "_0": "An array of all election addresses"
        }
      },
      "getElection(uint256)": {
        "details": "Get an election address by index",
        "params": {
          "_index": "Index in the elections array"
        },
        "returns": {
          "_0": "The address of the election at the specified index"
        }
      },
      "getElectionCount()": {
        "details": "Get the number of elections created",
        "returns": {
          "_0": "The total number of elections"
        }
      }
    }
